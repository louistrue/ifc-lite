    },
    "parquet-wasm": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "license": "MPL-2.0",
  "author": "Louis True",
  "repository": {
    "type": "git",
    "url": "https://github.com/louistrue/ifc-lite.git",
    "directory": "packages/server-client"
  },
  "homepage": "https://louistrue.github.io/ifc-lite/",
  "bugs": "https://github.com/louistrue/ifc-lite/issues",
  "keywords": [
    "ifc",
    "bim",
    "server",
    "client",
    "api",
    "aec"
