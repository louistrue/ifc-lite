{
  "name": "@ifc-lite/server-bin",
  "version": "1.11.0",
  "description": "Pre-built IFC-Lite server binary - run without Docker or Rust",
  "type": "module",
  "bin": {
    "ifc-lite-server": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "postinstall": "node dist/postinstall.js || true"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "tar": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "license": "MPL-2.0",
  "author": "Louis True",
  "repository": {
    "type": "git",
    "url": "https://github.com/louistrue/ifc-lite.git",
    "directory": "packages/server-bin"
  },
  "homepage": "https://louistrue.github.io/ifc-lite/",
  "bugs": "https://github.com/louistrue/ifc-lite/issues",
  "keywords": [
    "ifc",
    "bim",
    "server",
    "binary",
    "cli",
    "aec"
  ],
  "publishConfig": {
    "access": "public"
  }
}
